{
    //  TLS fuzzer specific configuration
    //  Disables ASAN due to fiber/stack conflicts with Address Sanitizer
    enable: 'manual',
    inherit: ['compiler'],
    environment: {
        //  Disable sanitizers for TLS fuzzer
        //  ASAN has issues with fiber context switching
        SANITIZE: '0',
    },
    execution: {
        workers: 1,
        timeout: 36000,
    },
    services: {
        cleanup: './cleanup.sh',
        prep: './prep.sh',
        setup: './setup.sh',
        healthCheck: {
            type: 'http',
            url: 'http://localhost:4200',
            timeout: 120,
            interval: 500,
        },
    },
}
